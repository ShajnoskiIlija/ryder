<!-- component -->
<body >
        <div class="flex flex-col flex-1 w-full overflow-y-auto">
          <main class="relative z-0 flex-1 pb-8 px-6 bg-white">
              <div class="grid pb-10  mt-4 ">
                  <!-- Start Content-->
                    <div class="mb-2">
                      <p class="text-lg font-semibold text-gray-400">Status:</p>
                    </div>
                    <div class="grid grid-cols-12 gap-6 border-b-2 pb-5">
                      <div class="col-span-12 sm:col-span-12 md:col-span-8 lg:col-span-8 xxl:col-span-8">
                        <div class="grid grid-cols-1 gap-2 sm:grid-cols-2 md:grid-cols-5 lg:grid-cols-5 xl:grid-cols-5 mt-3">
                           <div class="p-4">
                              <p class="text-xl font-bold">Current Number:</p>
                                <%= link_to @count_rent, dashboard_rent_items_path, class:"text-xl font-semibold filter drop-shadow"%>
                              <p class="text-xs font-semibold text-gray-400">Items for rent</p>
                           </div>
                           <div class="p-4">
                              <p class="text-xl font-bold">Bicycles:</p>
                                <%= link_to @bicycle, '#', class:"text-xl font-semibold filter drop-shadow"%>
                              <p class="text-xs font-semibold text-gray-400">
                           </div>
                           <div class="p-4">
                              <p class="text-xl font-bold">Electric Scooters:</p>
                                <%= link_to @elec_scooter, '#', class:"text-xl font-semibold filter drop-shadow"%>
                              <p class="text-xs font-semibold text-gray-400">Items</p>
                           </div>
                           <div class="p-4">
                              <p class="text-xl font-bold">Summer Equipment </p>
                                <%= link_to @summer_eq, '#', class:"text-xl font-semibold filter drop-shadow"%>
                              <p class="text-xs font-semibold text-gray-400">Items</p>
                           </div>
                           <% if @winter_eq.nil? %>
                            <div class="p-4">
                              <p class="text-xl font-bold">Winter Equipment </p>
                                  <%= link_to @winter_eq, '#', class:"text-xl font-semibold filter drop-shadow"%>
                                  <p class="text-xs font-semibold text-gray-400"></p>
                                <% else %>
                                  <p class="text-xl font-bold">Winter Equipment </p>
                                    <%= link_to @winter_eq, new_dashboard_rent_item_path, class:"text-xl font-semibold filter drop-shadow hover:bg-gray-500"%>
                                  <p class="text-xs font-semibold text-gray-400">Create New</p>
                            <% end %>
                            </div>
                        </div>
                      </div>

                    </div>
                    <div class="grid grid-cols-1 gap-2 sm:grid-cols-1 md:grid-cols-3 lg:grid-cols-3 xl:grid-cols-3 mt-3">
                      <div class="relative w-full h-52 bg-cover bg-center group rounded-lg overflow-hidden shadow-lg transition duration-300 ease-in-out"
                        style="background-image: url('https://images-wixmp-ed30a86b8c4ca887773594c2.wixmp.com/f/f868ecef-4b4a-4ddf-8239-83b2568b3a6b/de7hhu3-3eae646a-9b2e-4e42-84a4-532bff43f397.jpg?token=eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9..R0h-BS0osJSrsb1iws4-KE43bUXHMFvu5PvNfoaoi8o');">
                          <div class="absolute inset-0 bg-pink-900 bg-opacity-75 transition duration-300 ease-in-out"></div>
                          <div class="relative w-full h-full px-4 sm:px-6 lg:px-4 flex items-center justify-center">
                            <div>
                              <h3 class="text-center text-white text-lg">
                                   Items for rent
                              </h3>
                              <h3 class="text-center text-white text-3xl mt-2 font-bold">
                                   <%= link_to @count_rent, dashboard_rent_items_path, class:"text-xl font-semibold filter drop-shadow hover:bg-red-500"%>
                              </h3>
                              <div class="flex space-x-4 mt-4">
                                 <%= link_to "My Rents", dashboard_rent_items_path, class:"block uppercase mx-auto shadow bg-white text-indigo-600 focus:shadow-outline focus:outline-none text-white text-xs py-3 px-4 rounded font-bold"%>
                                <%= link_to "Create New", new_dashboard_rent_item_path, class:"block uppercase mx-auto shadow bg-indigo-800 hover:bg-indigo-700 focus:shadow-outline focus:outline-none text-white text-xs py-3 px-4 rounded font-bold"%>
                              </div>
                            </div>
                          </div>
                      </div>
                       <div class="relative w-full h-52 bg-cover bg-center group rounded-lg overflow-hidden shadow-lg transition duration-300 ease-in-out"
                        style="background-image: url('https://images-wixmp-ed30a86b8c4ca887773594c2.wixmp.com/f/f868ecef-4b4a-4ddf-8239-83b2568b3a6b/de7hhu3-3eae646a-9b2e-4e42-84a4-532bff43f397.jpg?token=eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9..R0h-BS0osJSrsb1iws4-KE43bUXHMFvu5PvNfoaoi8o');">
                          <div class="absolute inset-0 bg-yellow-600 bg-opacity-75 transition duration-300 ease-in-out"></div>
                            <div class="relative w-full h-full px-4 sm:px-6 lg:px-4 flex items-center">
                            <div>
                              <div class="text-white text-lg flex space-x-2 items-center">
                                <div class="bg-white rounded-md p-2 flex items-center">
                                  <i class="fas fa-toggle-off fa-sm text-yellow-300"></i>
                                </div>
                                <p>Finished Appt</p>
                              </div>
                              <h3 class="text-white text-3xl mt-2 font-bold">
                                  120
                              </h3>
                               <h3 class="text-white text-lg mt-2 text-yellow-100 ">
                                  4 not confirmed
                              </h3>
                            </div>
                          </div>
                      </div>
                       <div class="relative w-full h-52 bg-cover bg-center group rounded-lg overflow-hidden shadow-lg transition duration-300 ease-in-out"
                        style="background-image: url('https://images-wixmp-ed30a86b8c4ca887773594c2.wixmp.com/f/f868ecef-4b4a-4ddf-8239-83b2568b3a6b/de7hhu3-3eae646a-9b2e-4e42-84a4-532bff43f397.jpg?token=eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9..R0h-BS0osJSrsb1iws4-KE43bUXHMFvu5PvNfoaoi8o');">
                          <div class="absolute inset-0 bg-blue-900 bg-opacity-75 transition duration-300 ease-in-out"></div>
                          <div class="relative w-full h-full px-4 sm:px-6 lg:px-4 flex items-center">
                            <div>
                              <div class="text-white text-lg flex space-x-2 items-center">
                                <div class="bg-white rounded-md p-2 flex items-center">
                                  <i class="fas fa-clipboard-check fa-sm text-blue-800"></i>
                                </div>
                                <p>Finished Appt</p>
                              </div>
                              <h3 class="text-white text-3xl mt-2 font-bold">
                                  72
                              </h3>
                               <h3 class="text-white text-lg mt-2 ">
                                 3.4% <span class='font-semibold text-blue-200'>vs last month</span>
                              </h3>
                            </div>
                          </div>
                      </div>
                    </div>

                  <!-- End Content-->
              </div>
          </main>
        </div>
    </div>
</body>